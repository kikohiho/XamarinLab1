<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Audity.Views.ReceiptsLogPage">
  <StackLayout>
    <Button></Button>
    <ActivityIndicator IsRunning="" IsVisible=""></ActivityIndicator>
    <ListView x:Name="receiptsListView" ItemSource="{Binding Receipts}">
      <ListView.ItemTemplate>
        <DataTemplate>
          <TextCell Text="{Binding titulo}" Detail="{Binding costo}" />
        </DataTemplate>
      </ListView.ItemTemplate>
    </ListView>
  </StackLayout>
 
</ContentPage>